


digraph "package dependencies"
{
    node [shape=box]
    compound=true;
    
    subgraph cluster_log {
        node [style=filled];
    	"org.apache.logging.log4j:log4j-api@2.13.1"
    	"org.apache.logging.log4j:log4j-core:jar:2.13.1"
        "com.example:logconfig:1.0.1"
    	label = "log";
    }
    
    subgraph cluster_foo {
        node [style=filled];
        
        
        "com.example:foo@1.0.1";
        "com.example:greeter@1.0.1" [label=<com.example:greeter@<font color='red'>1.0.1</font>>];
        
        "com.example:foo@1.0.1" -> "org.apache.logging.log4j:log4j-api@2.13.1";
        "com.example:foo@1.0.1" -> "com.example:greeter@1.0.1";
    }
    
    subgraph cluster_bar {
        node [style=filled];
        
        "com.example:greeter@2.0.0" [label=<com.example:greeter@<font color='red'>2.0.0</font>>];
        "com.example:bar@1.0.1" -> "org.apache.logging.log4j:log4j-api@2.13.1";
        "com.example:bar@1.0.1" -> "com.example:greeter@2.0.0";
    }
    
    subgraph cluster_app {
        node [style=filled];
        
        "com.example:app@1.0.1"  -> "com.example:foo@1.0.1";
        "com.example:app@1.0.1"  -> "com.example:bar@1.0.1";
        
        "com.example:app@1.0.1"  -> "com.example:foo@1.0.1" [ltail=cluster_app,lhead=cluster_foo];
        "com.example:app@1.0.1"  -> "com.example:bar@1.0.1" [ltail=cluster_app,lhead=cluster_bar];
    }
}